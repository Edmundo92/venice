{
  "name": "venice-jsm",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/venice-jsm/venice-jsm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --no-open",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "format": "npx prettier --write \"{,!(node_modules)/**/}*.{js,jsx}\"",
    "fix": "npx eslint --fix \"{,!(node_modules)/**/}*.{js,jsx}\"",
    "storybook": "start-storybook -s dist/",
    "all": "npm-run-all --parallel start storybook"
  },
  "devDependencies": {
    "@babel/core": "7.8.6",
    "@stencil/core": "^1.9.0-14",
    "@stencil/react-output-target": "0.0.3",
    "@storybook/addon-actions": "5.3.14",
    "@storybook/addon-backgrounds": "5.3.14",
    "@storybook/addon-jest": "5.3.14",
    "@storybook/addon-notes": "5.3.14",
    "@storybook/addon-options": "5.3.14",
    "@storybook/addon-viewport": "5.3.14",
    "@storybook/html": "5.3.14",
    "babel-loader": "8.0.6",
    "copy-webpack-plugin": "5.1.1",
    "npm-run-all": "4.1.5",
    "write-file-webpack-plugin": "4.5.1"
  },
  "license": "MIT",
  "dependencies": {
  }
}
