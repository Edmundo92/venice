{
  "name": "react-ds",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "start-storybook -p 9009 -c .storybook",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-storybook": "build-storybook -c .storybook -o ../../dist/react",
    "loki-test": "loki test",
    "loki-update": "loki update"
  },
  "dependencies": {
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "devDependencies": {
    "@storybook/preset-create-react-app": "^2.1.1",
    "@storybook/react": "^5.3.17",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^10.0.1",
    "@testing-library/user-event": "^10.0.0",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.12.31",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@venice/core": "1.0.0",
    "@venice/styles": "1.0.0",
    "babel-loader": "^8.1.0",
    "eslint-plugin-react": "^7.19.0",
    "react-is": "^16.13.1",
    "react-scripts": "3.4.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "loki": {
    "reactUri": "file:../../dist/react",
    "diffingEngine": "gm",
    "requireReference": true,
    "chromeDockerWithoutSeccomp": true,
    "chromeTolerance": 0.8,
    "chromeFlags": "--headless --disable-gpu --hide-scrollbars --no-sandbox",
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.app",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      }
    }
  }
}
